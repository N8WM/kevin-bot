{
  "$schema": "https://json.schemastore.org/package",
  "name": "kevin-bot",
  "version": "1.0.0",
  "description": "Miscellaneous Discord bot",
  "main": "dist/app/index.js",
  "scripts": {
    "dev": "tsx watch src/app/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node .",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev && npx prisma generate",
    "prisma:deploy": "npx prisma migrate deploy && npx prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nmccutchen/dva-bot-v2.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "discord.js": "^14.20.0",
    "dotenv": "^16.5.0",
    "node-cron": "^4.2.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/node": "^24.0.3",
    "eslint": "^9.29.0",
    "prisma": "^6.16.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.42.0"
  }
}
